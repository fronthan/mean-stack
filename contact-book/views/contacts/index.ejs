<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <%- include('../partials/head') %>
</head>
<body>
    <%- include('../partials/nav') %>

    <div class="contact contact-index container">
        <h2>Index</h2>
        <ul class="list-group">
            <% contacts.forEach(function(contact){ %>
                <li class="list-group-item">
                    <a href="/contacts/<%= contact._id %>">
                        <%= contact.name %>
                    </a>
                </li>
            <% }) %>
        </ul>
    </div>
</body>
</html>